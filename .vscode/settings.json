{
  "luau-lsp.completion.autocompleteEnd": true,
  "luau-lsp.completion.imports.enabled": true,
  "luau-lsp.completion.imports.requireStyle": "alwaysRelative",
  "luau-lsp.completion.imports.separateGroupsWithLine": true,
  "luau-lsp.completion.imports.stringRequires.enabled": true,
  "luau-lsp.diagnostics.strictDatamodelTypes": true,
  "luau-lsp.diagnostics.workspace": true,
  "luau-lsp.hover.multilineFunctionDefinitions": true,
  "luau-lsp.hover.showTableKinds": true,
  "luau-lsp.platform.type": "roblox",
  "luau-lsp.sourcemap.enabled": true,
  "luau-lsp.sourcemap.sourcemapFile": "./.meta/sourcemap.json",
  "luau-lsp.sourcemap.generatorCommand": "pesde run sourcemap_generator > .meta/sourcemap.json",
  "luau-lsp.types.robloxSecurityLevel": "RobloxScriptSecurity",
  "files.exclude": {
    "luau_packages": true,
    "lune_packages": true,
    "roblox_packages": true
  },
  "luau-lsp.ignoreGlobs": [
    "/Packages/**",
    "/*_packages/**",
    "/dist.lua"
  ],
  "luau-lsp.fflags.enableNewSolver": true,
  "luau-lsp.types.definitionFiles": {
    "@custom": "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.4/luau_defs/types/environment.d.luau",
    "@luarmor": "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.4/luau_defs/types/luarmor.d.luau",
    "@luraph": "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.4/luau_defs/types/luraph.d.luau"
  },
  "luau-lsp.types.documentationFiles": [
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.4/luau_defs/docs/environment.d.json",
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.4/luau_defs/docs/luarmor.d.json",
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.4/luau_defs/docs/luraph.d.json"
  ],
  "selene.configPath": "./.meta/selene.toml",
  "stylua.configPath": "./.meta/.stylua.toml",
  "workbench.editor.customLabels.patterns": {
    "**/init.lua": "${dirname} (${filename}.${extname})",
    "**/init.luau": "${dirname} (${filename}.${extname})"
  },
  "files.associations": {
    ".luaurc": "json"
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
    "editor.formatOnSave": false,
    "editor.tabSize": 4,
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false
  },
  "[luau]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false
  },
  "[luaurc]": {
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false
  },
  "[toml]": {
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false
  },
  "editor.insertSpaces": true,
  "files.eol": "\n"
}